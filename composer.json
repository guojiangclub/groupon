{
  "name": "ibrand/groupon",
  "type": "library",
  "description": "iBrand Groupon component",
  "keywords": [
    "iBrand",
    "Groupon",
    "component"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shjchen",
      "email": "ibrand.shjchen@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "prettus/l5-repository": "^2.1",
    "ibrand/laravel-shopping-cart": "~1.0",
    "ibrand/laravel-scheduling": "~1.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "iBrand\\Component\\Groupon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iBrand\\Component\\Groupon\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "iBrand\\Component\\Groupon\\GrouponServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "repositories": [
    {
      "packagist": false
    },
    {
      "type": "composer",
      "url": "http://packagist.phpcomposer.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
